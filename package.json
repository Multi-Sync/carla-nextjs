{
  "name": "carla-nextjs",
  "version": "1.0.0",
  "description": "Next.js plugin for Carla AI assistant with automatic API route scanning and tool generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "carla-nextjs": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "nextjs",
    "ai",
    "assistant",
    "carla",
    "interworky",
    "mcp",
    "realtime-api",
    "openai",
    "chatbot"
  ],
  "author": "Interworky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Multi-Sync/carla-nextjs.git"
  },
  "bugs": {
    "url": "https://github.com/Multi-Sync/carla-nextjs/issues"
  },
  "homepage": "https://github.com/Multi-Sync/carla-nextjs#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "glob": "^10.3.10",
    "inquirer": "^9.2.0",
    "ora": "^5.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.10.0",
    "@types/ora": "^3.2.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}
